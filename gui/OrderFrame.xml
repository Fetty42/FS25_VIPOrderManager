<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<GUI onOpen="onOpen" onClose="onClose" onCreate="onCreate">
    <Bitmap profile="dialogFullscreenBg" id="dialogBg" />
    <GuiElement profile="fs25_dialogBg" id="dialogElement" size="1350px 800px">        
        <ThreePartBitmap profile="fs25_dialogBgMiddle" />
        <ThreePartBitmap profile="fs25_dialogBgTop" />
        <ThreePartBitmap profile="fs25_dialogBgBottom" />
        <GuiElement profile="fs25_dialogContentContainer">
            <Text profile="dialogTitle" text="$l10n_ui_orderDlg_title" id="dialogTitleElement" position="0px -30px" />

            <!-- Main list -->
            <Bitmap profile="ingameMenuMyList" id="tableHeaderBox" position="50px -90px" size="1230px 50px">
                <Text profile="ingameMenuMyHeaderLeft" position="10px -0px" size="360px 40.77px" text="$l10n_ui_orderDlg_header_product"/>
                <Text profile="ingameMenuMyHeaderRight" position="400px -0px" size="150px 40.77px" text="$l10n_ui_orderDlg_header_requested"/>
                <Text profile="ingameMenuMyHeaderRight" position="550px -0px" size="150px 40.77px" text="$l10n_ui_orderDlg_header_delivered"/>
                <Text profile="ingameMenuMyHeaderRight" position="700px -0px" size="150px 40.77px" text="$l10n_ui_orderDlg_header_payout"/>
                <Text profile="ingameMenuMyHeaderLeft" position="890px -0px" size="320px 40.77px" text="$l10n_ui_orderDlg_header_station"/>
                <ThreePartBitmap profile="fs25_lineSeparatorBottom" position="0px 2px" />
            </Bitmap>

            <SmoothList profile="ingameMenuMyList" id="orderTable" focusInit="onOpen" focusChangeTop="nil" focusChangeBottom="nil" selectedWithoutFocus="false" listSectionHeader="section" position="50px -145px" size="1230px 550px">
                <ListItem profile="ingameMenuMyListItem"  name="orderRowTemplate" size="1230px 40.77px">
                    <Bitmap profile="ingameMenuMyRowCellImage" name="fillTypeIcon" />
                    <Text profile="ingameMenuMyRowCellLeft" position="45px 0px" size="325px 40.77px" name="ftTitle" />
                    <Text profile="ingameMenuMyRowCellRight" position="400px 0px" size="150px 40.77px" name="quantity" textAlignment="right"/>
                    <Text profile="ingameMenuMyRowCellRight" position="550px 0px" size="150px 40.77px" name="fillLevel" textAlignment="right"/>
                    <Text profile="ingameMenuMyRowCellRight" position="700px 0px" size="150px 40.77px" name="payout" textAlignment="right"/>
                    <Text profile="ingameMenuMyRowCellLeft" position="890px 0px" size="320px 40.77px" name="targetStationTitle" textLayoutMode="scrolling"/>
                </ListItem>

                <ListItem profile="ingameMenuMyListItem" name="section" imageColor="0 0 0 0.35">
                    <Text profile="ingameMenuMyRowSectionLeft" name="title" textSize="18px" size="355px 40.77px"/>
                </ListItem>
            </SmoothList>

            <ThreePartBitmap profile="fs25_listSliderBox" position="-30px -55px" size="12px 550px" with="anchorTopRight">
                <Slider profile="fs25_listSlider" size="6px 542px" dataElementId="orderTable" id="tableSlider" />
            </ThreePartBitmap>  
        </GuiElement>

        <!-- Buttons -->
        <BoxLayout profile="fs25_dialogButtonBox" size="1250px 60px">
            <Button profile="buttonBack" text="$l10n_ui_orderDlg_btnClose" onClick="onClickBack" />
            <Button profile="buttonExtra1" text="$l10n_ui_orderDlg_btnAbort" onClick="onClickAbort" id="buttonAbort" />
            <Button profile="buttonOK" text="$l10n_ui_orderDlg_btnTagSellPoint" onClick="onTagLocation" id="buttonTag" />
            <Button profile="buttonActivate" text="$l10n_ui_orderDlg_btnAvailableTypes" onClick="onShowAvailableTypes" id="buttonShowTypes" />
            <!-- buttonActivate -->
            <!-- buttonSelect -->
        </BoxLayout>
    </GuiElement>
</GUI>